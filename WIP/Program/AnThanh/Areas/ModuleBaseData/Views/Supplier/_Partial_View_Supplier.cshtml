@using ObjInfo.Nation;
@using AnThanh.Commons;
@using ObjInfo.ModuleBaseData;
@{
    List<NationInfo> _listNation = DataMemory.GetNation();
    ViewBag.Title = "Thêm mới";
}
@{
    SupplierInfo supplier = new SupplierInfo();
    if (ViewBag.Supplier != null)
    {
        supplier = ViewBag.Supplier;
    }
}

<link href="~/Scripts/ComboCheckList/multiple-select.css" rel="stylesheet" />
<script src="~/Scripts/ComboCheckList/jquery.multiple.select_new.js"></script>

<style>
    .ms-drop {
        position: fixed;
        width: 52.8% !important;
        top: 332.766px !important;
        left: 326px !important;
    }

    .ms-parent {
        display: inline-block;
        position: relative !important;
        vertical-align: middle;
    }

    .ms-choice {
        border-radius: 0px;
    }
</style>

@try
{
    <div class="d-main">
        <div class="" style="padding:5px 38px 27px 38px">
            <form id="_FormRegisterSupplier">
                <div class="divCover">
                    <div class="div-search-col">
                        <div class="div-search-title-N">
                            <span>Mã nhà cung cấp*</span>
                        </div>

                        <input id="txt_id_edit" value="@supplier.Id" class="display-none" />
                        <div class="div-search-ad">
                            <div class="div-search-content">
                                <div>
                                    <input data-controlcondition="TRUE" type="text" id="txt_supplier_code_edit" class="input_search" maxlength="100" value="@supplier.Code" readonly="readonly" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="div-search-col">
                        <div class="div-search-title-N">
                            <span>Tên nhà cung cấp*</span>
                        </div>
                        <div class="div-search-ad">
                            <div class="div-search-content">
                                <div>
                                    <input data-controlcondition="TRUE" type="text" id="txt_name_edit" class="input_search" maxlength="500" value="@supplier.Name" readonly="readonly" />
                                </div>
                            </div>
                        </div>
                    </div>
                   
                    <div class="div-search-col-2">
                        <div class="div-search-col floatleft">
                            <div class="div-search-title-N">
                                <span>Quốc gia*</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="" class="input_search" maxlength="500" value="@supplier.Name_Nation" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="div-search-col-50 floatright width49">
                            <div class="div-search-title-N">
                                <span>Số hợp đồng</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="" class="input_search" maxlength="50" value="@supplier.Number_Contract.ToString("#,##0")" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="div-search-col-2">
                        <div class="div-search-col floatleft">
                            <div class="div-search-title-N">
                                <span>Số điện thoại*</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="phone_edit" class="input_search" maxlength="50" value="@supplier.Phone" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="div-search-col-50 floatright width49">
                            <div class="div-search-title-N">
                                <span>Số Fax</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="faxedit" class="input_search" maxlength="50" value="@supplier.Fax" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="div-search-col-2">
                        <div class="div-search-col floatleft">
                            <div class="div-search-title-N">
                                <span>Email</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="email_edit" class="input_search" maxlength="50" value="@supplier.Email" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="div-search-col-50 floatright width49">
                            <div class="div-search-title-N">
                                <span>Website</span>
                            </div>
                            <div class="div-search-ad">
                                <div class="div-search-content">
                                    <div>
                                        <input data-controlcondition="TRUE" type="text" id="website_edit" class="input_search" maxlength="50" value="@supplier.Website" readonly="readonly" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="div-search-col">
                        <div class="div-search-title-N">
                            <span>Ghi chú</span>
                        </div>

                        <div class="div-search-ad">
                            <div class="div-search-content">
                                <div>
                                    <input data-controlcondition="TRUE" type="text" id="txt_note_edit" class="input_search" maxlength="500" value="@supplier.Notes" readonly="readonly" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group-bottom">
                    <input type="button" class="btn-default" value="Thoát" onclick="Close()" style="width: auto;" />
                </div>
            </form>
        </div>
    </div>
   
}
catch (Exception ex)
{

}


